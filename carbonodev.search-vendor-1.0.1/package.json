{
	"name": "search-vendor",
	"displayName": "Search vendor",
	"description": "Quickly search the vendor folder.",
	"license": "MIT",
	"version": "1.0.1",
	"publisher": "carbonodev",
	"engines": {
		"vscode": "^1.5.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.search_vendor"
	],
	"main": "./extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.search_vendor",
				"title": "Search vendor"
			}
		],
		"keybindings": [
			{
				"command": "extension.search_vendor",
				"key": "ctrl+k ctrl+v",
				"mac": "cmd+k cmd+v",
				"when": "!terminalFocus"
			}
		],
		"configuration": {
			"title": "Search vendor",
			"properties": {
				"search-vendor.useLastFolder": {
					"type": "boolean",
					"default": false,
					"description": "Default to folder of last opened file when searching."
				},
				"search-vendor.path": {
					"type": "string",
					"default": "vendor",
					"description": "Relative path to vendor folder."
				}
			}
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"lint": "eslint ."
	},
	"devDependencies": {
		"@jasonnutter/eslint-config": "^1.0.1",
		"@types/node": "^6.0.40",
		"babel-eslint": "^7.1.1",
		"eslint": "^3.12.2",
		"eslint-config-airbnb": "^13.0.0",
		"eslint-plugin-import": "^2.2.0",
		"eslint-plugin-jsx-a11y": "^2.2.3",
		"eslint-plugin-mocha": "^4.8.0",
		"eslint-plugin-react": "^6.8.0",
		"pre-commit": "^1.2.2",
		"typescript": "^2.0.3",
		"vscode": "^1.1.10"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/CarbonoDev/vscode-search-vendor"
	},
	"icon": "img/composer-logo.jpg",
	"pre-commit": [
		"lint"
	],
	"__metadata": {
		"id": "9599c03d-f58b-4128-bb57-b28527423728",
		"publisherId": "4de77532-50c0-4ef1-b839-65d7179c0ce5",
		"publisherDisplayName": "Carbono Dev"
	}
}